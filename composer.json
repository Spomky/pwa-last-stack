{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "babdev/pagerfanta-bundle": "^4.0",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.16",
        "knplabs/knp-time-bundle": "dev-main",
        "pagerfanta/doctrine-orm-adapter": "^4.0",
        "pagerfanta/twig": "^4.0",
        "spomky-labs/pwa-bundle": "^1.1",
        "survos/pwa-extra-bundle": "^1.5.175",
        "symfony/asset": "6.4.*",
        "symfony/asset-mapper": "6.4.*",
        "symfony/console": "^6.4",
        "symfony/dotenv": "^6.4",
        "symfony/flex": "^2",
        "symfony/form": "^6.4",
        "symfony/framework-bundle": "^6.4",
        "symfony/monolog-bundle": "^3.0",
        "symfony/runtime": "^6.4",
        "symfony/security-csrf": "^6.4",
        "symfony/stimulus-bundle": "2.x-dev",
        "symfony/twig-bundle": "^6.4",
        "symfony/ux-autocomplete": "2.x-dev",
        "symfony/ux-live-component": "2.x-dev",
        "symfony/ux-turbo": "2.x-dev",
        "symfony/ux-twig-component": "2.x-dev",
        "symfony/validator": "^6.4",
        "symfony/web-link": "6.4.*",
        "symfony/yaml": "^6.4",
        "symfonycasts/dynamic-forms": "dev-main",
        "symfonycasts/tailwind-bundle": "dev-main",
        "tales-from-a-dev/flowbite-bundle": "dev-main",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "config": {
        "platform": {},
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.4.*",
            "docker": true
        }
    },
    "require-dev": {
        "dbrekelmans/bdi": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "6.4.*",
        "symfony/css-selector": "6.4.*",
        "symfony/debug-bundle": "^6.4",
        "symfony/filesystem": "6.4.*",
        "symfony/maker-bundle": "^1.51",
        "symfony/mime": "6.4.*",
        "symfony/panther": "^2.1",
        "symfony/phpunit-bridge": "7.1.x-dev",
        "symfony/stopwatch": "^6.4",
        "symfony/web-profiler-bundle": "^6.4",
        "zenstruck/browser": "1.x-dev",
        "zenstruck/foundry": "^1.36"
    }
}
